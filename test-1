
##Question no 1
def maximumsteal(values):
    if not values:
        return 0
    if len(values) == 1:
        return values[0]
    temp = [0] * len(values)
    temp[0] = values[0]
    temp[1] = max(values[0], values[1])
    for value in range(2, len(values)):
        temp[value] = max(temp[value-1],temp[value-2] + values[value])
    
    return temp[-1]
values = [6, 7, 1, 3, 8, 2, 5]
print(maximumsteal(values))
